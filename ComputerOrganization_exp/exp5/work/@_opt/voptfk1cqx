library verilog;
use verilog.vl_types.all;
entity ALU is
    port(
        clk             : in     vl_logic;
        r_in1           : in     vl_logic_vector(2 downto 0);
        r_in2           : in     vl_logic_vector(2 downto 0);
        m_in            : in     vl_logic_vector(2 downto 0);
        alu_op          : in     vl_logic_vector(6 downto 0);
        \out\           : out    vl_logic_vector(2 downto 0)
    );
end ALU;
